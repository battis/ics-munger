{
    "name": "battis/ics-munger",
    "description": "Reshape ICS feeds in useful ways",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Seth Battis",
            "email": "seth@battis.net"
        }
    ],
    "require": {
        "kigkonsult/icalcreator": "^2.4",
        "doctrine/orm": "^2.6",
        "michelf/php-markdown": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Battis\\IcsMunger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Battis\\IcsMunger\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "marcuspi/random-factor": "dev-master"
    },
    "scripts": {
        "phpcs": "phpcs --standard=phpcs.xml"
    }
}
